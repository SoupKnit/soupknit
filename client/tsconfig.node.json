{
  "extends": "../tsconfig.base.node.json",
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noEmit": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
    },
  },
  "include": [
    "vite.config.ts"
  ]
}