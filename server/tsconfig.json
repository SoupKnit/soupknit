{
  "extends": [
    "../tsconfig.base.node.json",
    "fastify-tsconfig"
  ],
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "./dist",
    "noImplicitAny": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "target": "ES2021",
    "lib": [
      "es2021"
    ],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./src/**/*.ts",
    "src/core/pythonSandbox/run.ts"
  ]
}